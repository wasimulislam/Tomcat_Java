 list acclaimnum
        table acreqheader
        order xacreqnum
        select "xacreqnum like '%"+zvalue+"%' and (left(xacreqnum,4)='ECF-')"
		searchselect "left(xacreqnum,4)='ECF-'"
        rows 20
        objects xacreqnum attrib(link "abc" embed onclick="return updateParent(this:'')"),~
                xdate,xnote

     end list


