 list poreqdocnum
        table poreqheader
        order xporeqnum desc
        select "xporeqnum like '%"+zvalue+"%' and left(xporeqnum,4) in ('DOC-')"
		searchselect "left(xporeqnum,4) in ('DOC')"
        rows 20
        objects xporeqnum attrib(link "abc" embed onclick="return updateParent(this:'')"),~
                xtwh,xregi,xdate

     end list


